		"./node": "./src/lib/Node.js",
		"./stack": "./src/lib/RunTimeStack.js",
		"./fetcher": "./src/lib/Fetcher.js",
		"./fetcherinc": "./src/lib/FetcherInc.js",
		"./getter": "./src/lib/Getter.js",
		"./tree": "./src/lib/TreeMapper.js",
		"./taml": "./src/lib/TamlMapper.js",
