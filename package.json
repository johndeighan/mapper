		"url": "https://github.com/johndeighan/string-input/issues"
	},
	"homepage": "https://github.com/johndeighan/string-input#readme",
	"devDependencies": {
		"ava": "^3.15.0",
		"coffeescript": "^2.6.1"
	},
	"dependencies": {
		"@jdeighan/coffee-utils": "^4.0.15",
		"cross-env": "^7.0.3",
		"marked": "^3.0.7",
		"sass": "^1.43.3"
