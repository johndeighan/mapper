		"url": "git+https://github.com/johndeighan/mapper.git"
	},
	"homepage": "https://github.com/johndeighan/mapper#readme",
	"devDependencies": {
		"@jdeighan/unit-tester": "^2.0.0",
		"coffeescript": "^2.7.0"
	},
	"dependencies": {
		"@jdeighan/coffee-utils": "^7.0.25",
		"cross-env": "^7.0.3",
		"marked": "^4.0.14",
		"sass": "^1.51.0"
	}
