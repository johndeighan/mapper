{
	"name": "@jdeighan/string-input",
	"type": "module",
	"engines": {
		"node": ">=12.0.0"
	},
	"version": "8.0.6",
	"description": "A class to help parse text files",
	"main": "./StringInput.js",
	"exports": {
		".": "./src/StringInput.js",
		"./tree": "./src/TreeWalker.js",
		"./get": "./src/Getter.js",
		"./taml": "./src/taml.js",
		"./sass": "./src/sass.js",
		"./markdown": "./src/markdown.js",
		"./coffee": "./src/coffee.js"
	},
	"scripts": {
